{
  "name": "sr-message-maker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --port 9001 --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "update-data": "tsx update && prettier -w ./src/assets/data/static",
    "init-update": "playwright install chromium"
  },
  "dependencies": {
    "@ckpack/parameter": "^2.9.0",
    "@marshallswain/vuedraggable": "^4.2.0",
    "html-to-image": "^1.11.11",
    "mitt": "^3.0.1",
    "star-rail-vue": "^1.3.9",
    "super-image-cropper": "^1.0.20",
    "vue": "^3.5.8",
    "vue-picture-cropper": "^0.7.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.19.50",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "npm-run-all": "^4.1.5",
    "p-limit": "^7.0.0",
    "playwright": "^1.54.2",
    "prettier": "^3.3.3",
    "prettier-plugin-stylus-format": "^1.0.1",
    "stylus": "^0.63.0",
    "tsx": "^4.20.4",
    "typescript": "^5.6.2",
    "unplugin-auto-import": "^0.17.8",
    "vite": "^5.4.7",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-vue-devtools": "^7.4.5",
    "vue-tsc": "^2.1.6"
  }
}
